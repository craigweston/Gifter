DirectoryIndex index.php
RewriteEngine on
RewriteCond $1 !^(index\.php|ckeditor|images|css|flowplayer|javascript|uploads|robots\.txt|favicon\.ico)
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ ./index.php/$1 [L,QSA]

<Files ~ "^.*\.([Hh][Tt][Aa])">
 order allow,deny
 deny from all
 satisfy all
</Files>

ErrorDocument 500 /pagenotfound
ErrorDocument 404 /pagenotfound
ErrorDocument 401 /pagenotfound
ErrorDocument 403 /pagenotfound